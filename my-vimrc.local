	set cscopetag
        filetype plugin indent on
        syn on se title
        set tabstop=8
        set softtabstop=8
        set shiftwidth=8
        set noexpandtab
        set fileencodings=utf-8,gb2312,gbk,gb18030
        set termencoding=utf-8
        syntax enable
        set background=dark
        colorscheme solarized
        map <F3> :NERDTreeMirror<CR>
        map <F3> :NERDTreeToggle<CR>
        map <F9> :TagbarToggle<CR>
